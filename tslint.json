{
  "extends": [
    "tslint-ionic-rules"
  ],
  "rules": {
    "align": [
      true,
      "statements"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-var-keyword": true
  }
}